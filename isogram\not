#include<string.h>
void main()
{
	char b[500];
	int i,j,l,count=0;
	printf("Enter the String:");
	scanf("%s",b);
	l=strlen(b);
	for(i=0;i<l;i++)
	{
		for(j=i+1;j<=l;j++)
		{
			if(b[i]==b[j])
			{
				count++;
			}
		}
	}
	if(count==0)
	{
		printf("\nIsogram");
	}
	else
	{
		printf("\nNot a Isogram");
	}
}
